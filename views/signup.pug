            //source(src="video/websitewarehouse4.webm" type="video/webm")
        #home-overlay-2
    // End of Build section
    // Interactive Map
    section#pricing
        .content-box-md-interact
            div
                .map-color-3
                    .row
                        .home-headings.tools-p-align
                            .horizontal-heading(style="margin-bottom: 2px; margin-top: -10px;")
                                h1.home-headings-2(style="margin-bottom: 2px; padding: 0;")
                                    span(style="font-size: 20px; padding: 0;")  Multi-Page Templates
                .map-color-6
                    .container
                        .row
                            // Site content
                            #content
                                section#map-section.inner.over
                                    ul#examples-2(style="margin-top: 60px;")
                                        li
                                            a.background-map-2(href='/') ‚Üê Back to All Templates
                                    .desc.map-color-7.container-paragraph.login-css-responsive-1(style="width: 600px;margin: 70px auto; height: 550px;")
                                        .map-paragraph
                                            h2.i-map-paragraph
                                                b Sign Up For An Account
                                            br
                                            // Contact
                                            section#contact
                                                div
                                                    .container(style="padding-right: 0; padding-left: 0;")
                                                        .row
                                                            //
                                                            .col-md-12(style="padding-left: 0;")
                                                                // Contact Right
                                                                #contact-right.form-styles-c(style="height: 400px")
                                                                    form.form2(style="width: 500px; height: 400px")
                                                                        // <h4 class="form-t-c">Send Us A Message</h4>
                                                                        .row
                                                                            .col-md-12.col-sm-12
                                                                                .form-group
                                                                                    p(style=" margin-bottom: 5px;") Enter your Name
                                                                                    input#name.form-control.login-css-responsive-4(type="name", required, placeholder="Name")
                                                                            .col-md-12.col-sm-12
                                                                                .form-group
                                                                                    p(style="margin-bottom: 5px;") Enter your Email
                                                                                    input#email.form-control.login-css-responsive-4(type="email", required, placeholder="you@example.com")
                                                                            .col-md-6.col-sm-6
                                                                                .form-group
                                                                                    p(style="margin-bottom: 5px;") Enter your Password
                                                                                    input#password.form-control.login-css-responsive-4(type="password", minlength='8', required, placeholder="*********")
                                                                            .col-md-6.col-sm-6
                                                                                .form-group
                                                                                    p(style="margin-bottom: 5px;") Confirm your Password
                                                                                    input#passwordConfirm.form-control.login-css-responsive-4(type="password", minlength='8', required, placeholder="Confirm Your Password")
                                                                        #submit-btn.form-submit-b.login-css-responsive-3(style="text-align: center; margin-top: 20px;")
                                                                            button.btn.btn-general.btn-yellow Sign Up
                                                                            a.btn.btn-general.btn-yellow(href='/login' style="color: #4c6085; background-color: transparent;" type="submit" name="submit" role="button") Login
                                            // End of Contact
    // End of Pricing
