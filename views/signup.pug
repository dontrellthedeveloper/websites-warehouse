extends base

block content
    include _header
    // BG Video
    section#home-4
        // <img src="img/about/gear2.svg" alt id="leftgear">
        // <img src="img/about/gear2.svg" alt id="rightgear">
        // Background Video
        video#home-bg-video-2(poster="video/websitewarehouse8.jpg" autoplay="" loop="" muted="" playsinline="")
            //source(src="video/websitewarehouse4.mp4" type="video/mp4")
