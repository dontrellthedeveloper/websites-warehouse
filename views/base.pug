                                    li
                                        a.smooth-scroll.ipad-m(href="#onepage-templates") Beauty
                                    li
                                        a.smooth-scroll.ipad-m(href="#onepage-templates") Fitness/Health
                                    li
                                        a.smooth-scroll.ipad-m(href="#onepage-templates") Design
                                    li
                                        a.smooth-scroll.ipad-m(href="#onepage-templates") Event
                                    li
                                        a.smooth-scroll.ipad-m(href="#onepage-templates") Restaurant
                                    li
                                        a(href="index.html#tools") Login
        // End of Header
    // Home
    section#home
        // Background Video
        video#home-bg-video(poster="video/websitewarehouse4.jpg" autoplay="" loop="" muted="" playsinline="")
            source(src="video/websitewarehouse4.mp4" type="video/mp4")
            source(src="video/websitewarehouse4.ogv" type="video/ogg")
            source(src="video/websitewarehouse4.webm" type="video/webm")
        // Overlay
        #home-overlay
        // Home Content
        #home-content
            #home-content-inner.text-center
                .slider-text
                    .desc
                        a.smooth-scroll(href="#onepage-templates")
                            span.subheading Websites
                            h1.home-heading-style-main
                                span.web-home-style Websites
                                br
                                |  Warehouse
                            span.subheading-2 Made Easy
        // Arrow Down
        a#arrow-down.smooth-scroll(href="#templates")
            i.fa.fa-angle-down
    // Home End
    // Website Templates
    #templates
        .websites-portfolio
            section#onepage-templates.page2.portfolio-style.port-bg-color-2
                .map-color-5
                    .row
                        .home-headings.tools-p-align
