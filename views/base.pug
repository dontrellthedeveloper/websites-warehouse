        header
            nav.navbar.navbar-fixed-top.mobile-nav-top
                .container-fluid
                    .site-nav-wrapper
                        // Main Menu
                        .container
                            .navbar-header
                                span#mobile-nav-open-btn ☰
                                a.navbar-brand(href="index.html")
                                    img.img-rsz(src="img/logo/websites-warehouse-logo-light-new.png" width="80px" height="80px" alt="logo")
                            .collapse.navbar-collapse
                                ul.nav.navbar-nav.pull-right.wh
                                    li
                                        a(href="index.html") Home
                                    li
                                        a.ipad-m(href="#") Websites
                                        ul.nav-sub-style
                                            li
                                                a.smooth-scroll.ipad-m(href="#onepage-templates") Business
                                            li
                                                a.smooth-scroll.ipad-m(href="#onepage-templates") Clothing
                                            li
                                                a.smooth-scroll.ipad-m(href="#onepage-templates") Beauty
                                            li
                                                a.smooth-scroll.ipad-m(href="#onepage-templates") Fitness/Health
                                            li
                                                a.smooth-scroll.ipad-m(href="#onepage-templates") Design
                                            li
