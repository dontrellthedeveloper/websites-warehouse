                                            li
                                                a.smooth-scroll.ipad-m(href="#onepage-templates") Restaurant
                                    li
                                        a(href="index.html#tools") Login
                        // Mobile Menu
                        #mobile-nav
                            // Mobile Menu Close Button
                            span#mobile-nav-close-btn ×
                            #mobile-nav-content
                                ul.nav
                                    li
                                        a(href="/") Home
                                    li
                                        a.smooth-scroll.ipad-m(href="#onepage-templates") Business
                                    li
